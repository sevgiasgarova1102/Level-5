Level 2 task 2.
#include <iostream>
using namespace std;

void ededYoxla(int a) {
    if (a > 0) {
        cout << "Eded musbetdir" << endl;
    } 
    else if (a < 0) {
        cout << "Eded menfidir" << endl;
    } 
    else {
        cout << "Eded sifirdir" << endl;
    }
}
int main() {
    int a;
    cin >> a;
    ededYoxla(a);
   return 0;
}

Level 2 task 5.
#include <iostream>
using namespace std;

void imtahanBal(int bal) {
    if (bal >= 0 && bal <= 50)
        cout << "F";
    else if (bal >= 51 && bal <= 60)
        cout << "E";
    else if (bal >= 61 && bal <= 70)
        cout << "D";
    else if (bal >= 71 && bal <= 80)
        cout << "C";
    else if (bal >= 81 && bal <= 90)
        cout << "B";
    else if (bal >= 91 && bal <= 100)
        cout << "A";
}

int main() {
    int bal;
    cout << "BalÄ± daxil edin: "; 
    cin >> bal;
    imtahanBal(bal);
    return 0;
}
